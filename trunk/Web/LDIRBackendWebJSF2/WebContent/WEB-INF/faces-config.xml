<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<application>
		<locale-config>
			<default-locale>ro</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<application>
		<view-handler>org.ajax4jsf.application.AjaxViewHandler</view-handler>
	</application>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.RegisterBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>voteGarbageManagerBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.VoteGarbageManagerBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>adminUserSessionBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.AdminUserSessionBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>teamBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.TeamManagerBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orgBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.OrganizationManagementBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mormanManager</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.MormanManagerBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userManager</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.UserManagerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>areaManager</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.AreaManagerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mapViewBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.MapViewBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>areaCleanManager</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.AreaCleanManagerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resetBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.ResetBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminUsersManagerBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.AdminUsersManagerBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminGarbageManagerBean</managed-bean-name>
		<managed-bean-class>ro.radcom.ldir.ldirbackendwebjsf2.managedBeans.AdminGarbageManagerBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>

	</managed-bean>

</faces-config>
